{
    "name": "eleven-labs/api-validator",
    "description": "Validate PSR-7 Requests against an OpenAPI/Swagger2 Schema",
    "authors": [
        {
            "name": "Guillem CANAL",
            "email": "gcanal@eleven-labs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ElevenLabs\\Api\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElevenLabs\\Api\\Validator\\": "tests/"
        }
    },
    "require": {
        "psr/http-message": "^1.0",
        "symfony/yaml": "^2.7|^3.0",
        "symfony/config": "^2.7|^3.0",
        "justinrainbow/json-schema": "^2.0",
        "rize/uri-template": "^0.3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "guzzlehttp/psr7": "^1.3"
    }
